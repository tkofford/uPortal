  <!--
    Group: "DefaultUser"
    Checks that the user at least has a uid, isn't one of the following (AdmittedStudent, OrientationStudent, EnrollingStudent), 
    and not DPP, if so they're a part of the group.
  -->
  <group>
    <group-key>DefaultUser</group-key>
    <group-name>DefaultUser</group-name>
    <group-description>Users who are not (AdmittedStudent, OrientationStudent, EnrollingStudent), and not DPP</group-description>
    <selection-test>
      <test-group>
        <!--                <test>-->
        <!--                    <attribute-name>uid</attribute-name>-->
        <!--                    <tester-class>org.jasig.portal.groups.pags.testers.ValueExistsTester</tester-class>-->
        <!--                    <test-value/>-->
        <!--                </test>-->
        <test>
          <attribute-name>kuPersonAffiliation</attribute-name>
          <tester-class>org.jasig.portal.groups.pags.testers.RegexTester</tester-class>
          <test-value>^(?:(?!\b(?:Admitted|Orientation|Enrolling)Student\b).)*$</test-value>
        </test>
        <test>
          <attribute-name>kuPersonAffiliation</attribute-name>
          <tester-class>org.jasig.portal.groups.pags.testers.RegexTester</tester-class>
          <test-value>^(?!DP).+</test-value>
        </test>
      </test-group>
    </selection-test>
  </group>

